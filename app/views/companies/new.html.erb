<div class='row'>
  <h1>Register Company</h1>
</div>

<%= form_for @company do |f| %>
  <div class='row'>
    <%= f.label :name %>
  </div>
  <div class='row'>
    <%= f.text_field :name, autofocus: true %>
  </div>

  <div class='row'>
    <%= f.submit 'Submit', class: 'radius button' %>
  </div>
<% end %>
